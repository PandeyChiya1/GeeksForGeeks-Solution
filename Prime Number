https://www.geeksforgeeks.org/problems/prime-number2314/1?page=1&difficulty=Basic&sortBy=submissions

class Solution {
    public static int largest(int[] arr) {
        // code here
        int max = arr[0];
        
        for(int i =0; i<arr.length; i++){
            if(max<arr[i]){
                max = arr[i];
            }
            
            
        }
        return max;
    }
}
